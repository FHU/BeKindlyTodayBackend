{
  "name": "bekindlytodaybackend",
  "version": "1.0.0",
  "description": "The backend for the bekindlytoday app",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "dotenv -e ./.env.dev nodemon ./server.ts",
    "spin-db": "dotenv -e ./.env.dev docker compose up",
