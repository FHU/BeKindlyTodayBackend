  "devDependencies": {
    "@prisma/client": "^5.11.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "dotenv-cli": "^7.4.1",
    "express": "^4.19.1",
    "nodemon": "^3.1.0",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
